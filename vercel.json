{
  "version": 2,
  "builds": [
    {
      "src": "sportAppFrtEnd/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "sportAppRestApi/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "sportAppRestApi/$1" },
    { "src": "/(.*)", "dest": "sportAppFrtEnd/$1" }
  ]
}